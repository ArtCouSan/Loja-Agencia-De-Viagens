// Querys de busca cliente

- SELECT COUNT(*) AS CAMPO FROM CLIENTE WHERE CAMPO = BUSCA AND ativo = TRUE; CONTA NUMERO DE RESULTADOS
- SELECT COUNT(*) AS CAMPO FROM CLIENTE WHERE CAMPO = BUSCA AND ativo = FALSE; CONTA NUMERO DE RESULTADOS
- SELECT COUNT(*) AS CAMPO FROM CLIENTE WHERE ATIVO = TRUE;
- SELECT COUNT(*) AS CAMPO FROM CLIENTE WHERE ATIVO = FALSE;
